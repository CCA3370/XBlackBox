{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "XBlackBox Viewer",
  "version": "1.0.0",
  "identifier": "com.xblackbox.viewer",
  "build": {
    "frontendDist": "../static",
    "devUrl": "http://localhost:8080",
    "beforeDevCommand": "",
    "beforeBuildCommand": ""
  },
  "app": {
    "windows": [
      {
        "title": "XBlackBox Viewer",
        "width": 1400,
        "height": 900,
        "resizable": true,
        "fullscreen": false,
        "center": true,
        "minWidth": 1024,
        "minHeight": 768
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
